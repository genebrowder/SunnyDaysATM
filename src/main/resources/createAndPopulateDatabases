show databases;

create database sunny_days_atm;

use sunny_days_atm;

CREATE TABLE user_login_info
(
    user_login_info_id INT(11) PRIMARY KEY AUTO_INCREMENT NOT NULL,
    account_number VARCHAR(10) NOT NULL,
    pin VARCHAR(4) NOT NULL
);

show tables;


desc `user_log_info`;

insert into user_login_info (account_number, pin)
values("123456","4321")  ;
