values("234567","4321")  ;
insert into user_login_info (account_number, pin)
values("345678","4321")  ;
insert into user_login_info (account_number, pin)
values("456789","4321")  ;
insert into user_login_info (account_number, pin)
values("567890","4321")  ;

select * from user_log_info;

drop table  user_login_info;

create table `user`(
  `user_id` INT(11)     NOT NULL AUTO_INCREMENT,
  `user_login_info_id`  INT(11)   NOT NULL,
  `last_name`      VARCHAR(30) NOT NULL,
  `first_name`      VARCHAR(30) NOT NULL,
  `city`      VARCHAR(30) NOT NULL,
  `state`      VARCHAR(2) NOT NULL,
  `zip_code`      VARCHAR(5) NOT NULL,
  `phone_number`      VARCHAR(10),

  PRIMARY KEY (`user_id`),
  FOREIGN KEY (`user_login_info_id`) REFERENCES user_login_info(`user_login_info_id`)
) ;



insert into user (user_login_id, last_name, first_name, city, state, zip_code, phone_number)
values("1","B","A","Saint Louis","MO","63000","3141234567");
